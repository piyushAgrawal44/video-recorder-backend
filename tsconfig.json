  "compilerOptions": {
    "target": "ES2024",
    "lib": ["es6"],
    "module": "commonjs",
    "rootDir": "./src",                        
    "resolveJsonModule": true,                         
    "outDir": "./dist",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": true,   
    "skipLibCheck": true
  }
}
